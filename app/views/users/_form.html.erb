<div class="col-md-7 ">
<%= form_for(user,validate:true) do |form| %>
 
  <div class="form-group">
    <%= form.label :first_name,"First Name" %>
    <%= form.text_field :first_name, id: :user_first_name ,:placeholder=>"First name",required: true,:class=>"form-control", validate: true%>
  </div>

  <div class="form-group">
    <%= form.label :last_name,"Last Name", class: "control-label" %>
    <%= form.text_field :last_name, id: :user_last_name, :placeholder=>"Last name",required: true,:class=>"form-control", validate: true %>
  </div>

  <div class="form-group">
    <%= form.label :age, class: " control-label" %>
    <%= form.number_field :age, id: :user_age,:placeholder=>"Age",:min=>"10",required: true,:class=>"form-control", validate: true %>
  </div>

  <div class="form-group">
    <%= form.label :gender, class: "control-label" %>
    <!--%= form.number_field :gender, id: :user_gender %-->
    <%= form.select :gender,User.genders.keys,{}, { class: "form-control",id: "placeholder-example",required: true,validate: true} %>
  </div>
  
 
  <div class="actions">
    <%= form.button "Submit", class: "btn btn-default",title:"Submit"%><span>
    <%= link_to "Back" ,users_path, class: "btn btn-default pull-right ",title:"Back"%></span>
  </div>
<% end %>
</div>
<script>
    $(document).ready(function() {
      $('select#placeholder-example').select2({
        placeholder: "Choose a Any one",
        allowClear: true
      });
    });
    </script>