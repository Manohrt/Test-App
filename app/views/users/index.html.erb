<div class="col-md-7 ">
<div class="table-responsive">
   <table class="table">

   <h4 class="panel-title h-arrow"><h1>Users Details</h1><span class="badge1"><%= link_to new_user_path,class: "btn btn-default pull-right" , :title => 'New' do%><i class="fa fa-plus-square"></i><%end%></span></h4>
     <thead>
     <tr>
     <th>#</th>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
      <th colspan="3"></th>
     </tr>
     </thead>

    <tbody>
    <%a=((params[:page] || 1).to_i - 1) * 10%>
    <% @users.each_with_index do |user,i| %>
    <tr>
    <td><%= a+i+1 %></td>
    <td><%= user.full_name %></td>
    <td><%= user.age %></td>
    <td><%= user.gender %></td>
    <td><center><%= link_to user, class: "btn btn-default",:title => 'Show' do%><i class="fa fa-eye"></i><%end%></center></td>
    <td><center><%= link_to edit_user_path(user), class: "btn btn-default",:title => 'Edit' do%><i class="fa fa-pencil-square"></i><%end%></center></td>
    <td><center><%= link_to user, method: :delete, data: { confirm: 'Are you sure ?' } ,class: "btn btn-default",:title => 'Delete' do%><i class="fa fa-trash"></i><%end%></center></td>
   
    </tr>
    <% end %>
    </tbody>
   
    <!--%= link_to 'New User', new_user_path %-->
</table>
<div class="digg_pagination">
      <%= page_entries_info @users,:class=>"page_info" %>
    <div class="pull-right" >
    <%= will_paginate @users, :container => false%>
  </div>
  </div>
</div>
</div>
